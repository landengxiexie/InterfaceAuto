<suite name="集中告警-告警配置-降噪策略" verbose="1" parallel="false" thread-count="1">
    <test name="告警合并规则" preserve-order="true">
        <classes>
            <class name="com.testcases.VicubeSystem">
                <methods>
                    <include name="vicubeLogin"/>
                    <include name="promptMessage"/>
                    <include name="concentratedAlarm"/>
                    <include name="alarmConfig"/>
                    <include name="denoiseStrategy"/>
                    <include name="denoiseStrategyChooseAlarmMergeRules"/>
                    <include name="alarmMergeRulesCreateBasicSetting"/>
                    <include name="alarmMergeRulesCreateRulesConditionSetting"/>
                    <include name="alarmMergeRulesCreateRulesConditionAdvancedSetting"/>
                    <include name="alarmMergeRulesCreateAlarmMergeSetting"/>
                    <include name="denoiseStrategyAlarmMergeRulesSelectVerify"/>

                    <include name="alarmMergeRulesEditBasicSetting"/>
                    <include name="alarmMergeRulesEditRulesConditionSetting"/>
                    <include name="alarmMergeRulesEditRulesConditionAdvancedSetting"/>
                    <include name="alarmMergeRulesEditAlarmMergeSetting"/>
                    <include name="denoiseStrategyAlarmMergeRulesEditSelectVerify"/>

                    <include name="alarmRulesStartUsing"/>
                    <include name="denoiseStrategyAlarmMergeRulesEditSelect1"/>
                    <include name="alarmRulesBlockUp"/>
                    <include name="denoiseStrategyAlarmMergeRulesEditSelect"/>
                    <include name="alarmRulesDelete"/>

                </methods>
            </class>
        </classes>
    </test>
    <test name="告警分类规则" preserve-order="true">
