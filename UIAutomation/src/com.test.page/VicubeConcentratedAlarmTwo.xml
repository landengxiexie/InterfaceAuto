                    <include name="denoiseStrategy"/>
                    <include name="denoiseStrategyChooseAlarmClassifyRules"/>
                    <include name="alarmClassifyRulesCreateBasicSetting"/>
                    <include name="alarmClassifyRulesCreateRulesConditionSetting"/>
                    <include name="alarmClassifyRulesCreateRulesConditionAdvancedSetting"/>
                    <include name="alarmClassifyRulesCreateAlarmClassifySetting"/>
                    <include name="denoiseStrategyAlarmClassifyRulesSelectVerify"/>

                    <include name="alarmClassifyRulesEditBasicSetting"/>
                    <include name="alarmClassifyRulesEditRulesConditionSetting"/>
                    <include name="alarmClassifyRulesEditRulesConditionAdvancedSetting"/>
                    <include name="alarmClassifyRulesEditAlarmClassifySetting"/>
                    <include name="denoiseStrategyAlarmClassifyRulesEditSelectVerify"/>

                    <include name="alarmRulesStartUsing"/>
                    <include name="denoiseStrategyAlarmClassifyRulesEditSelect"/>
                    <include name="alarmRulesBlockUp"/>
                    <include name="denoiseStrategyAlarmClassifyRulesEditSelect1"/>
                    <include name="alarmRulesDelete"/>
                </methods>
            </class>
        </classes>
    </test>
